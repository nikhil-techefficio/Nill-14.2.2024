// Function to extract unique keys from JSON objects
function extractKeys(data) {
  let keys = {};
  data.forEach(obj => {
    for (let key in obj) {
      keys[key] = true;
    }
  });
  return Object.keys(keys);
}

// Function to populate the table with JSON data
function populateTable(data) {
  let tableHeaders = document.getElementById('tableHeaders');
  let tableBody = document.getElementById('tableBody');

  // Clear existing headers and body
  tableHeaders.innerHTML = '';
  tableBody.innerHTML = '';

  // Extract keys and create table headers
  let keys = extractKeys(data);
  keys.forEach(key => {
    let th = document.createElement('th');
    th.textContent = key;
    tableHeaders.appendChild(th);
  });

  // Populate table body
  data.forEach(obj => {
    let tr = document.createElement('tr');
    keys.forEach(key => {
      let td = document.createElement('td');
      if (key === 'states') {
        // Create a dropdown for the "states" array
        let select = document.createElement('select');
        obj[key].forEach(state => {
          let option = document.createElement('option');
          option.value = state.id;
          option.textContent = state.name;
          select.appendChild(option);
        });
        td.appendChild(select);
      } else if (typeof obj[key] === 'object') {
        // Handle other nested objects like timezones and translations
        td.textContent = JSON.stringify(obj[key]);
      } else {
        td.textContent = obj[key];
      }
      tr.appendChild(td);
    });
    tableBody.appendChild(tr);
  });
}

// Initial table population
populateTable(jsonData);

</script>

</body>
</html>
